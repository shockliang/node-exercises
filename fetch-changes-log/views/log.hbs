<!DOCTYPE html>

<table class="pane" style="border:dotted ">
    <tbody>
        {{#each items}}
        <tr class="pane">
            <td colspan="3" class="changeset"><a name="detail0"></a>
                <div class="changeset-message">
                    <b>修訂版次
                    <a>{{revision}}</a>
                    by <a>{{author.fullName}}</a>:
                    </b><br>{{msg}}
                </div>
            </td>
        </tr>
        <tr>
            <th style="text-align: left;">Change Type</th>
            <th style="text-align: left;">Path in Repository</th>
            <th style="text-align: left;">Path in Workspace</th>
        </tr>
            {{#each paths}}
            <tr>
                <td><a>{{editType}}</a></td>
                <td><a>{{file}}</a></td>
                <td>{{affectedPath}}</td> 
            
            </tr>
            {{/each}}
        {{/each}}
    </tbody>
</table>

